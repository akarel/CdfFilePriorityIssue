@using ClientDependency.Core.Mvc

@{
    // Does NOT work
    //Html.RequiresJs("~/Scripts/Script1.js", FilePriority.Ten)
    //    .RequiresJs("~/Scripts/Script2.js", FilePriority.Twenty);

    // Works!
    Html.RequiresJs("~/Scripts/Script1.js", (int)FilePriority.Ten)
        .RequiresJs("~/Scripts/Script2.js", (int)FilePriority.Twenty);
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>CDF Issue with File Priority</title>
    <script type="text/javascript" src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.js"></script>
    <style type="text/css">
        body {
            font-family: sans-serif;
        }
        h1 {
            padding: 20px;
        }
        div {
            margin: 20px;
        }
        #put-stuff-here {
            padding: 20px;
            border: 2px solid blue;
        }
        #view-content {
            padding: 20px;
            border: 2px solid red;
        }
    </style>
</head>
<body>
    <h1>Load Order</h1>

    <div id="put-stuff-here"></div>

    <div id="view-content">
        @RenderBody()
    </div>

    @Html.RenderJsHere()
</body>
</html>
